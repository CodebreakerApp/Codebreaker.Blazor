@namespace CodeBreaker.UI
@typeparam T
@inherits CodeBreaker.UI.Shared.Components.Select.CodeBreakerSelectBase<T>

<MudSelect Value="@Value" ValueChanged="ValueChanged" Label="@Label">
    @foreach(var item in Items) {
        <MudSelectItem Value="@item.Value">
            @item.Key
        </MudSelectItem>
    }
</MudSelect>
