@inherits CodeBreaker.UI.Shared.Components.Radio.CodeBreakerRadioGroupBase<T>
@namespace CodeBreaker.UI
@typeparam T

 <MudRadioGroup SelectedOption="@Value" SelectedOptionChanged="@ValueChanged">
    @foreach(var item in Items)
    {
        <MudRadio Option="@item.Value" Color="Color.Primary">@item.Key</MudRadio>
    }
</MudRadioGroup>

