@inherits CodeBreaker.UI.Shared.Components.Progress.CodeBreakerProgressCircularBase<double>
@namespace CodeBreaker.UI

@if (Indeterminate)
{
    <MudProgressCircular Class="@Class" Indeterminate="true" />
}
else
{
    <MudProgressCircular Class="@Class" Min="Min" Max="Max" Value="Value" />
}
