@inherits CodeBreaker.UI.Shared.Components.Radio.CodeBreakerRadioGroupBase<string>
@namespace CodeBreaker.UI

<FluentRadioGroup @bind-Value="Value" @bind-Value:after="OnValueChanged">
    @foreach (var item in Items)
    {
        <FluentRadio Value="@item.Value">@item.Key</FluentRadio>
    }
</FluentRadioGroup>

